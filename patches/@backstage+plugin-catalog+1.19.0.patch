+import StorageIcon from '@material-ui/icons/Storage';
+
+const DEFAULT_CACHE_TTL = { seconds: 10 };
+const DEFAULT_BATCH_DELAY = { milliseconds: 50 };
+const DEFAULT_ICONS = {
+  api: ExtensionIcon,
+  component: MemoryIcon,
+  system: CategoryIcon,
