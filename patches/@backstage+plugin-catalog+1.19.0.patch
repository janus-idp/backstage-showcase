+  return {
+    async: options.async,
+    render: ({ entityRef, entity, context }) => {
+      const presentation = defaultEntityPresentation(
+        entity || entityRef,
+        context
+      );
+      return {
+        snapshot: presentation,
+        loadEntity: options.async
+      };
+    }
+  };
+}
