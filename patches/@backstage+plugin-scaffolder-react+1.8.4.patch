+import qs from 'qs';
+import { useState } from 'react';
+
+const useFormDataFromQuery = (initialState) => {
+  return useState(() => {
+    if (initialState) {
+      return initialState;
+    }
+    const query = qs.parse(window.location.search, {
+      ignoreQueryPrefix: true
+    });
+    try {
+      return JSON.parse(query.formData);
+    } catch (e) {
+      return {};
+    }
+  });
+};
+
+export { useFormDataFromQuery };
