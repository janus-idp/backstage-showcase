{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "persistent": true
    },
    "export-dynamic": {
      "outputs": ["dist-dynamic/**"],
      "dependsOn": ["//#tsc"]
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["//#tsc", "^build"]
    },
    "app#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "lint": {},
    "//#tsc": {
      "outputs": ["dist-types/**"],
      "dependsOn": ["^tsc"]
    },
    "//#prettier:fix": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".prettierrc.js",
    ".eslintrc.js",
    "tsconfig.json",
    "yarn.lock"
  ]
}
