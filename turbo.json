{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".prettierrc.js",
    ".eslintrc.js",
    "tsconfig.json",
    "yarn.lock"
  ],
  "tasks": {
    "export-dynamic": {
      "outputs": [
        "dist-dynamic/**"
      ],
      "dependsOn": [
        "//#tsc"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "//#tsc",
        "^build"
      ]
    },
    "app#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {},
    "//#tsc": {
      "outputs": [
        "dist-types/**"
      ],
      "dependsOn": [
        "^tsc"
      ]
    },
    "//#prettier:fix": {
      "cache": false
    }
  }
}
